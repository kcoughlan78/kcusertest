<h1>Users List</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Email</th>
    <th>View</th>
  </tr>

  <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= link_to "View Details", account_path %></td>
      </tr>
      <tr>
      </tr>
  <% end %>
</table>